# Firebase Configuration
# Get these values from your Firebase project settings
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_DATABASE_URL=https://your_project-default-rtdb.firebaseio.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google OAuth Configuration
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id.apps.googleusercontent.com

# OpenAI API Configuration
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-your_openai_api_key_here
EXPO_PUBLIC_OPENAI_API_KEY=sk-proj-your_openai_api_key_here
# Optional: Custom OpenAI API URL (defaults to official OpenAI endpoint)
# OPENAI_API_URL=https://api.openai.com/v1/chat/completions

# Expo Configuration
EXPO_NO_TYPESCRIPT_SETUP=1

# App Assets Configuration
# Replace with your actual logo/image URLs
APP_LOGO_URL=https://i.postimg.cc/Wbp09kB4/icon.png